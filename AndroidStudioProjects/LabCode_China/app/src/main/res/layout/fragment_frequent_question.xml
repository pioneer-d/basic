<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    tools:context=".navigation.more.frg.FrequentQuestionFragment"
    android:background="@color/white">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <ImageButton
            android:id="@+id/backButton"
            android:layout_width="11dp"
            android:layout_height="20dp"
            android:src="@drawable/ic_left_button"
            android:layout_marginTop="62dp"
            android:layout_marginLeft="26dp"
            android:background="@android:color/transparent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintLeft_toLeftOf="parent"/>


        <TextView
            android:layout_width="wrap_content"
            android:layout_height="26dp"
            android:text="자주묻는질문"
            android:textSize="17dp"
            android:textColor="#111111"
            android:textStyle="bold"
            android:layout_marginTop="61dp"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintRight_toRightOf="parent"
            />

        <ImageButton
            android:layout_width="68dp"
            android:layout_height="36dp"
            android:src="@drawable/ic_more_all_click"
            android:background="@android:color/transparent"
            android:id="@+id/allQuestion"
            android:layout_marginTop="136dp"
            android:layout_marginLeft="20dp"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintLeft_toLeftOf="parent"/>

        <ImageButton
            android:layout_width="96dp"
            android:layout_height="36dp"
            android:src="@drawable/ic_more_use_non_click"
            android:background="@android:color/transparent"
            android:id="@+id/howToUseQuestion"
            android:layout_marginTop="136dp"
            android:layout_marginLeft="96dp"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintLeft_toLeftOf="parent"/>

        <ImageButton
            android:layout_width="68dp"
            android:layout_height="36dp"
            android:src="@drawable/ic_more_tech_non_click"
            android:background="@android:color/transparent"
            android:id="@+id/techQuestion"
            android:layout_marginTop="136dp"
            android:layout_marginLeft="200dp"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintLeft_toLeftOf="parent"/>

        <ExpandableListView
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_marginTop="204dp"
            android:listSelector="@color/white"
            android:id="@+id/question_list"
            app:layout_constraintTop_toTopOf="parent"
            android:scrollbars="none"
            />

    </androidx.constraintlayout.widget.ConstraintLayout>


</FrameLayout>