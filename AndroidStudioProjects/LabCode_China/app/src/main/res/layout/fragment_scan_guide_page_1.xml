<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    tools:context=".navigation.more.frg.ScanGuidePage_1">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <ImageView
            android:id="@+id/scanGuid_main"
            android:layout_width="142dp"
            android:layout_height="34dp"
            android:src="@drawable/ic_scan_guide_1_main"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintRight_toRightOf="parent" />

        <ImageView
            android:id="@+id/scanGuid_sub"
            android:layout_width="239dp"
            android:layout_height="48dp"
            android:src="@drawable/ic_scan_guide_1_sub"
            android:layout_marginTop="12dp"
            app:layout_constraintTop_toBottomOf="@+id/scanGuid_main"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintRight_toRightOf="parent" />

        <ImageView
            android:layout_width="335dp"
            android:layout_height="251dp"
            android:src="@drawable/ic_scan_guide_1_image"
            android:layout_marginTop="42dp"
            app:layout_constraintTop_toBottomOf="@+id/scanGuid_sub"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintRight_toRightOf="parent"/>

    </androidx.constraintlayout.widget.ConstraintLayout>

</FrameLayout>