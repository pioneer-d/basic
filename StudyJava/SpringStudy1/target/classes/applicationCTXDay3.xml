<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

	<bean id="student" class="com.javalec.ex.Day3.Student" scope = "singleton">
		<constructor-arg value="동준영"></constructor-arg>
		<constructor-arg value="24"></constructor-arg>
	</bean>	

	<bean id="otherStudent" class="com.javalec.ex.Day3.OtherStudent">
		<constructor-arg value="김병수"></constructor-arg>
		<constructor-arg value="24"></constructor-arg>
	</bean>


</beans>
