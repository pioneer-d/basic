package day4;

public class MainClass {
	
	public static void main(String[] args) {
	
		AccessClass ac = new AccessClass();

		int a1 = ac.a;
		int b1 = ac.b;
		int c1 = ac.c;
		int d1 = ac.d;
		
		System.out.println(a1);
		System.out.println(b1);
		System.out.println(c1);
		System.out.println(d1);
		
}

}
