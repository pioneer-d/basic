package staticTest;

public class MainClass {	//static의 이해 
	
	public static void main(String[] args) {
		
		//현재 PapaPouch에는 200원이 있다.
		
		System.out.println("현재 아빠지갑에 남은 돈 : "+PapaPouch.MONEY);
		
		FirstChild fc = new FirstChild();
		fc.getMoney(100);
		System.out.println("현재 아빠지갑에 남은 돈 : "+PapaPouch.MONEY);
		
		SecondChild sc = new SecondChild();
		sc.getMoney(100);
		System.out.println("현재 아빠지갑에 남은 돈 : "+PapaPouch.MONEY);
		
		ThirdChild tc = new ThirdChild();
		tc.getMoney(100);
		System.out.println("현재 아빠지갑에 남은 돈 : "+PapaPouch.MONEY);
		
		
	}

}
