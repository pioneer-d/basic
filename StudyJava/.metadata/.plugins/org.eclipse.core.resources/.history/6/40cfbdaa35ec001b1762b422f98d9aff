package day8.exceptionHandling;

public class Exception1 {
	
	public static void main(String[] args) {
		
		int i = 10;
		int j = 0;
		
		System.out.println("i+j : "+i+j);
		System.out.println("i-j : "+(i-j));
		System.out.println("i*j : "+i*j);
		System.out.println("i%j : "+i%j);
		
	}

}
