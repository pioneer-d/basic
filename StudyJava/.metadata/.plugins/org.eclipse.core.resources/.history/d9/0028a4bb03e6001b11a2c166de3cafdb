package day2;

public class Array1 {	//배열 선언 2가지

	public static void main(String[] args) {
		
		//index선언과 동시에 value를 입력하는 방식
		int[] i = {1,2,3,4,5};
		
		//index선언 후 value를 따로 입력하는 방식
		int[] j = new int[5];
		j[0] = 1;
		j[1] = 2;
		j[2] = 3;
		j[3] = 4;
		j[4] = 5;
		
		System.out.print(i);
		System.out.print(j);
		
	}
}
