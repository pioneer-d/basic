package day4;

public class GiveMoneyClass {	//static의 이해 
	
	public static void main(String[] args) {
		
		//현재 PapaPouch에는 200원이 있다.
		
		FirstChild fc = new FirstChild();
		fc.getMoney(200);
		
		SecondChild sc = new SecondChild();
		sc.getMoney(100);
		
		ThirdChild tc = new ThirdChild();
		tc.getMoney(100);
		
		
	}

}
