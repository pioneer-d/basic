package day9.collection2;

import java.util.HashMap;

public class HashMapClass {

	public static void main(String[] args) {
		
		HashMap <Integer,String> hash = new HashMap <Integer,String>();
		
		//put get remove clear
		hash.put(0, "test0");
		hash.put(1, "test1");
		hash.put(2, "test2");
		

	}
}
