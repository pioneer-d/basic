package day9.hashSet;

public class Student {	//HashSet 이해하기.
	
	private String name;
	private int grade;
	
	public Student(String name, int grade) {
		this.name = name;
		this.grade = grade;
	}

	//main클래스에서 재정의한 이유를 살펴보자.
	@Override
		public String toString() {
			return name+" : "+grade;
		}
	
}
