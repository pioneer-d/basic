package day4.extendSuper;

public class ChildClass extends ParentClass {	//super의 이해
	
	public ChildClass() {
		// TODO Auto-generated constructor stub
	}
	
	@Override
	public void method1() {
		super.method1();	//이때 super는 this(자기객체)와 다르게 상위 객체를 찾는 키워드!
		System.out.println("자식이 재정의 한 메소드!");
	}

}
