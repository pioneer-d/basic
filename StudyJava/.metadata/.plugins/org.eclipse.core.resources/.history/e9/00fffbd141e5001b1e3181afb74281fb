	public static void main(String []args) {
		int i = 10;
		int j = 5;
		
		//»ê¼ú¿¬»ê
		System.out.println("µ¡¼À = "+(i + j));
		System.out.println("»¬¼À = "+(i - j));
		System.out.println("³ª´°¼À = "+(i/j));
		System.out.println("³ª´« ³ª¸ÓÁö = "+(i%j));
		
