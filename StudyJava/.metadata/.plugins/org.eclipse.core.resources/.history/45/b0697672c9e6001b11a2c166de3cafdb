package day3;

public class Class3 {	//필드, 생성자, getter, setter
	
	//이것들을 인스턴스 변수라고 한다.(필드)
	//private로 설정을 했기 때문에 외부에서 직접 접근 불가이다.
	private int age;
	private String name;
	private int num;
	
	public Class3(int age, String name, int num) {	
		this.age = age;
		this.name = name;
		this.num = num;
	}

	public int getAge() {	//public이므로 밖에서 age값을 얻어갈 수 있다.
		return age;
	}

	public void setAge(int age) {	//public이므로 밖에서 age값을 설정 할 수 있다.
		this.age = age;
	}

	public String getName() {
		return name;
	}

	public void setName(String name) {
		this.name = name;
	}

	public int getNum() {
		return num;
	}

	public void setNum(int num) {
		this.num = num;
	}
 
	
	
}
