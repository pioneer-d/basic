package day8.exceptionHandling;

public class TryCatch1 {

	public static void main(String[] args) {

		int i = 10;
		int j = 0;

		System.out.println("i+j : " + i + j);
		System.out.println("i-j : " + (i - j));
		System.out.println("i*j : " + i * j);
		try {
			System.out.println("i/j : " + i / j);
		} catch (Exception e) {
			System.out.println(e.getMessage());
			//e.getMessage는 무슨 오류인지 출력하는 메소드
		}
	}

}
