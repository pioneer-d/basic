package day9.threadClass;

public class MainClass {	//Runnable을 구현하지 않고 Thread클래스를 상속한 thread
	
	public static void main(String[] args) {
		
		Thread thread = new ThreadClass();
		thread.setName("B");
		thread.start();
		
		System.out.println(Thread.currentThread().getName());
		
	}

}
