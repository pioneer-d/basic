package day3;

public class Class3 {	//필드, 생성자, getter, setter
	
	//이것들을 인스턴스 변수라고 한다.(필드)
	private int age;
	private String name;
	private int num;
	
	public Class3(int age, String name, int num) {	
		this.age = age;
		this.name = name;
		this.num = num;
	}

	public int getAge() {
		return age;
	}

	public void setAge(int age) {
		this.age = age;
	}

	public String getName() {
		return name;
	}

	public void setName(String name) {
		this.name = name;
	}

	public int getNum() {
		return num;
	}

	public void setNum(int num) {
		this.num = num;
	}
 
	
	
}
